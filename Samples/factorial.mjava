class Factorial {
  public static void main () {
    Util ut;
    ut = new Util();
    ut.Fac(10);
  }
}

class Util {
  public void Fac(int num) {
    /* simple */
    // factorial example
    assert (num > -1);
    res = 1;
    int i;
    i = 0;
    while (i < num) {
      res = res * (i + 1);
      i = i + 1;
    }
    System.out.println(res);
  }
  int res;
}